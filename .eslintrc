{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": 0,
    "react/state-in-constructor": 0,
    "react/prop-types": 0,
    "no-use-before-define": 0,
    "no-console": 0
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint"
}
